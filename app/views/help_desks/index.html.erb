<h1>Listing help_desks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @help_desks.each do |help_desk| %>
      <tr>
        <td><%= help_desk.name %></td>
        <td><%= help_desk.type %></td>
        <td><%= link_to 'Show', help_desk %></td>
        <td><%= link_to 'Edit', edit_help_desk_path(help_desk) %></td>
        <td><%= link_to 'Destroy', help_desk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Help desk', new_help_desk_path %>
