<% provide(:title, 'Chemical Record') %>
<p id="notice"><%= notice %></p>

<h1><%="Chemical Record"%></h1>

<div id="tabledata">
<table class="table table-bordered">
    <tr> 
      <th>Chlorine(ppm)</th> 
      <td><%=@chemical_record.chlorine_ppm%></td> 
    </tr>
    <tr> 
      <th> Chlorine(orp)</th> 
      <td><%=@chemical_record.chlorine_orp%></td> 
    </tr>
    <tr> 
      <th>PH</th> 
      <td><%=@chemical_record.ph%> </td> 
    </tr>
    <tr> 
      <th>Alkalinity</th> 
      <td><%=@chemical_record.alkalinity%></td> 
    </tr>
    <tr> 
      <th>Calcium Hardness</th> 
      <td><%=@chemical_record.calcium_hardness%></td> 
    </tr>
  <tr> 
    <th>Pool Temp</th> 
    <td><%=@chemical_record.pool_temp%></td> 
  </tr>
  <tr> 
    <th>Air Temp</th> 
    <td><%=@chemical_record.air_temp%></td> 
  </tr>
  <tr> 
    <th>Date Created</th> 
    <td><%=@chemical_record.created_at%></td> 
  </tr>
</table>

<%= link_to 'Edit', edit_chemical_record_path(@chemical_record) %>

</div>